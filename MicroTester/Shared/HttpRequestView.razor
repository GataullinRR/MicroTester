@if (Request != null)
{
    <p>
        <span class="wrapped">Uri: @Request.URI</span>
    </p>
    <p>
        <HttpHeadersView Headers="Request.Headers" />
    </p>
    <p>
        <span class="wrapped">@Request.Body</span>
    </p>
}

@code {
    [Parameter]
    public HttpRequest? Request { get; set; }
}
